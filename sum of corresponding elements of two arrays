#include <stdio.h>
void main(){
	int a1[10],a2[10],sum[10];
	int i;
	printf("Enter first array:-\n");
	for(i=0;i<10;i++){
		printf("ar1[%d]=",i);
		scanf("%d",&a1[i]);
	}
	printf("Enter second array:-\n");
	for (i=0; i<10; i++){
		printf("ar2[%d]=",i);
		scanf("%d",&a2[i]);
	}
	for(i=0;i<10;i++){
		sum[i] = a1[i] + a2[i];
	}
	printf("Sum of arrays:-\n");
	for(i=0;i<10;i++){
		printf("sum[%d]=%d\n",i,sum[i]);
	}
}
